<h3 class="center">報名查詢</h3>
<hr class="hr">
<div class="row">
  <div class="col-md-6 col-md-offset-3">

    <%= form_tag registers_path,method: :get do %>
      <%= label :search,"請輸入E-mail" %>
      <%= text_field_tag :search, params[:search], placeholder:"E-mail", class: 'form-control' %>
      <%= label :n,"請輸入姓名" %>
      <%= text_field_tag :n, params[:n], placeholder:"姓名", class: 'form-control' %>
      <%=submit_tag "查詢", name: nil, class: "btn btn-lg btn-success" %>
    <% end %>
  </div>
</div>